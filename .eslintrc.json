{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
      "indent": 0,
      "linebreak-style": 0,
      "no-alert": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "no-bitwise": 0,
      "no-use-before-define": 0,
      "no-param-reassign": ["error", { "props": false }],
      "no-console": 0,
      "class-methods-use-this": 0,
      "no-mixed-operators": 0,
      "no-continue": 0,
      "prefer-const": 0
  },
  "globals": {
    "mat4": true,
    "vec3": true,
    "quat": true,
    "gl": true,
    "canvas": true,
    "mouseInfo": true,
    "currentlyPressedKeys": true
  }
}
